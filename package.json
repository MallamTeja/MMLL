{
  "name": "mmll",
  "version": "1.0.0",
  "description": "Machine Learning Lifecycle Management Platform",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "uvicorn --app-dir backend app.main:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "npm --prefix frontend start",
    "build": "npm --prefix frontend run build",
    "test": "concurrently \"npm run test:backend\" \"npm run test:frontend\"",
    "test:backend": "cd backend && pytest",
    "test:frontend": "cd frontend && npm test -- --watchAll=false",
    "install:all": "npm install && python3 -m pip install -r backend/requirements.txt && npm install --prefix frontend",
    "bootstrap": "npm run install:all",
    "start:all": "npm run install:all && npm run dev",
    "start": "npm run start:all",
    "clean": "rm -rf node_modules frontend/node_modules backend/__pycache__ frontend/build",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MallamTeja/MMLL.git"
  },
  "author": "MallamTeja",
  "license": "MIT",
  "keywords": [
    "machine-learning",
    "mlops",
    "fastapi",
    "react",
    "typescript",
    "python"
  ]
}
